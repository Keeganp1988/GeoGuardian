{
  "expo": {
    "name": "GeoGuardian",
    "slug": "circlelink-mobile",
    "version": "1.0.0",
    "icon": "./assets/GeoGuardianLogo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "geoguardian",
    "web": {
      "favicon": "./assets/GeoGuardianLogo.png"
    },
    "android": {
      "package": "com.company.CircleLink",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCO55aIvQaye9J7Z5AqCVYDPlY1mS4X5LY"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.company.CircleLink",
      "config": {
        "googleMapsApiKey": "AIzaSyCO55aIvQaye9J7Z5AqCVYDPlY1mS4X5LY"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "GeoGuardian needs your location for real-time tracking.",
          "locationAlwaysPermission": "GeoGuardian needs your location to share it with your circles."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/GeoGuardianLogo.png",
          "color": "#1E293B"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-font",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "b5c8dc54-7f92-47a2-98a1-347467bce14a"
      }
    },
    "owner": "keeganp"
  }
}
